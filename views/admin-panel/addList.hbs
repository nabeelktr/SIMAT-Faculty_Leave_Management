{{> loader}}
{{> admin-header}}

{{> admin-nav}}
<main class="mn-inner">
    <div class="row">
        <div class="col s12 m12 l6">
        <div class="card white darken-1" style="margin-left: 20%;">

            <div class="card-content ">
                <span class="card-title" style="font-size:20px;"></span></span>




                <section>
                    <container class="container mt-4 ">
                        <div class="row ">
                            <div class="col-md-12">
                                <form action="/admin/addList" method="post">
                                    <div class="col s6">

                                        <label for="">Id</label>
                                        <input type="text" name="id" class="form-control">
                                        <label for="">Name</label>
                                        <input type="text" name="name" class="form-control">





                                        <label for="">Dob</label>
                                        <input type="text" name="dob" class="form-control">

                                        <label for="">Phone</label>
                                        <input type="text" name="phone" class="form-control">
                                    </div>




                                    <div class=" col s6">
                                        <label for="">Department</label>
                                                <select name="department" autocomplete="off">
                                                            
                                                            <option value="cse">CSE</option>
                                                            <option value="me">ME</option>
                                                            <option value="ece">ECE</option>
                                                            <option value="eee">EEE</option>
                                                            <option value="ce">CE</option>
                                                            
                                                        </select>

                                        <label for="">Email</label>
                                        <input type="email" name="email" class="form-control">

                                        <label for="">Password</label>
                                        <input type="password" class="form-control" name="password"><br><br>


                                        <button type="submit" class="btn btn-primary form-control">Add
                                            List</button>






                                    </div>

                                </form>
                            </div>
                        </div>
                    </container>
                </section>







            </div>
        </div>
    </div>
</div>
</main>


<div class="left-sidebar-hover"></div>